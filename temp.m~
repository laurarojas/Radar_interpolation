clear all;
[van1_int R] = geotiffread('20008081635_van_sweep1_60min-morph-31.tiff');
[van1_real_miss R] = geotiffread('VAN_1800_sweep1.tiff');
[van1 R] = geotiffread('VAN_Z2_sweep1.tiff');
%[ker1 R] = geotiffread('KER_Z2_sweep1_wgs.tiff');
%[ker1_int R] = geotiffread('201008081645KER_20100808_sweep1-morph-16-wgs.tiff');
%ker1c = imresize(ker1,size(van1),'nearest');
%van1 = ker1_int;
%nan values at sameplace
%van1_int = van1_real_miss;
C = van1+van1_int;

van1 = C-van1_int;
van1_int = C-van1;

disp('')
van2 = van1(abs(van1) >= 0);
van2_int = van1_int(abs(van1_int)>=0);

corr(van2_int,van2)
