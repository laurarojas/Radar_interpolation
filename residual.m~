C = van1+van1_int;

van1 = C-van1_int;
van1_int = C-van1;

%van1(isnan(van1)) = [];
%van1_int(isnan(van1_int)) = [];

max_val = max([max(van1_int(:)) max(van1(:))]); %for nomalizatio

%van1_int = van1_int./max_val;
%van1 = van1./max_val;

tmp = (van1_int-van1).*(van1_int-van1);
